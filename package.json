{
  "name": "PetalDev",
  "version": "1.3.0",
  "description": "Douban.FM Client With Extra - - -",
  "main": "bundle/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron .",
    "dev:hot": "cross-env NODE_ENV=development webpack-dev-server --config webpack/config.dev.js",
    "bundle:main": "webpack --config webpack/config.electron.js --progress",
    "bundle:dev": "cross-env NODE_ENV=development webpack --config webpack/config.dev.js --progress",
    "bundle:dev+main": "npm run bundle:dev && npm run bundle:main",
    "bundle:prod": "cross-env NODE_ENV=production webpack --config webpack/config.prod.js --progress",
    "bundle:prod+main": "npm run bundle:prod && npm run bundle:main",
    "dist": "electron-builder --config electron-builder.json",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ilime/Petal.git"
  },
  "keywords": [
    "Douban.FM",
    "electron",
    "client"
  ],
  "author": "Yue Yang <g1enyy0ung@gmail.com> (http://g1eny0ung.site)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.0",
    "babel-jest": "^22.0.3",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "electron-builder": "^19.49.0",
    "electron-devtools-installer": "^2.2.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.14.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.0.3",
    "node-sass": "^4.7.1",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "moment": "^2.20.1",
