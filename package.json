{
  "name": "PetalDev",
  "version": "2.14.0",
  "description": "Douban.FM Client With Extra - - -",
  "main": "bundle/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron .",
    "dev:hot": "webpack-dev-server --config webpack/config.dev.js",
    "bundle:dev": "webpack --config webpack/config.dev.js --progress",
    "bundle:prod": "webpack --config webpack/config.prod.js --progress",
    "bundle:electron-dev": "cross-env NODE_ENV=development webpack --config webpack/config.electron.js --progress",
    "bundle:electron-prod": "cross-env NODE_ENV=production webpack --config webpack/config.electron.js --progress",
    "bundle:dev+electron-dev": "yarn run bundle:dev && yarn run bundle:electron-dev",
    "bundle:prod+elcetron-prod": "yarn run bundle:prod && yarn run bundle:electron-prod",
    "dist": "electron-builder --config electron-builder.json",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ilime/Petal.git"
  },
  "keywords": [
    "Douban.FM",
    "electron",
    "client"
  ],
  "author": "Yue Yang <g1enyy0ung@gmail.com> (http://g1eny0ung.site)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.1.4",
    "css-loader": "^1.0.1",
    "electron": "4.0.0",
    "electron-builder": "^20.36.2",
    "electron-devtools-installer": "^2.2.4",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.9.0",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.10.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "react-hot-loader": "^4.3.12",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.23.1",
