{
  "name": "petal",
  "version": "1.0.0",
  "description": "Douban.FM Client With Extra - - -",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "bundle:src": "webpack --config webpack.config.src.js --progress",
    "bundle:main": "webpack --config webpack.config.main.js --progress",
    "bundle:src+main": "npm run bundle:src && npm run bundle:main",
    "dev:watch": "webpack --config webpack.config.src.js --progress --watch",
    "dev:mainWatch": "webpack --config webpack.config.main.js --progress --watch",
    "bundle:pro": "webpack --config webpack.config.production.js --progress",
    "dist": "build",
    "test": "jest --silent"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SandStorms/Petal.git"
  },
  "keywords": [
    "Douban.FM",
    "electron",
    "client"
  ],
  "author": "SandStorms",
  "license": "MIT",
  "build": {
    "appId": "org.sandstorms.petal",
    "dmg": {
      "contents": [
        {
          "x": 100,
          "y": 180
        },
        {
          "x": 300,
          "y": 180,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "window": {
        "width": 400,
        "height": 280
      }
    },
    "win": {}
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.1",
    "electron": "^1.6.8",
    "electron-builder": "^17.5.0",
    "electron-devtools-installer": "^2.2.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "jest": "^20.0.1",
    "node-sass": "^4.5.2",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.1"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "moment": "^2.18.1",
    "nedb": "^1.8.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "semantic-ui-react": "^0.68.3"
  }
}