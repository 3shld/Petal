    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.1",
    "redux-cond": "^1.1.0",
    "redux-thunk": "^2.2.0",
    "semantic-ui-react": "^0.77.1"
  }
